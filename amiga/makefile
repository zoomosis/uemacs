#
#  Makefile for microEmacs 3.12 using Aztec
#
CFLAGS=+C +D +L

OFILES=amigados.o ansi.o basic.o bind.o buffer.o char.o crypt.o \
	display.o eval.o exec.o file.o fileio.o input.o isearch.o \
	line.o main.o mouse.o random.o region.o search.o replace.o \
	window.o word.o

emacs:	$(OFILES)
	ln -F emacs.lnk

main.o:	main.c
	cc $(CFLAGS) -Z6000 main.c
